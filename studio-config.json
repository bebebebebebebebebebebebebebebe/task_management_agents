{
  "server_url": "http://localhost:8000",
  "auth_mode": "development",
  "workspace": "task_management_agents",
  "auto_sync": true,
  "features": {
    "visual_debugging": true,
    "real_time_monitoring": true,
    "collaborative_editing": false,
    "version_control": true
  },
  "layouts": {
    "default": "hierarchical",
    "alternative": "force_directed",
    "compact": "circular"
  },
  "themes": {
    "default": "dark",
    "alternative": "light"
  },
  "visualization": {
    "node_spacing": 100,
    "edge_curve": "smooth",
    "animation_duration": 300,
    "show_labels": true,
    "show_types": true
  },
  "debugging": {
    "breakpoints_enabled": true,
    "state_inspection": true,
    "step_by_step": true,
    "performance_monitoring": true
  }
}
